<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.star.uml.designer.application.StarUMLApplication">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.star.uml.designer.ui.views.category.views.staruml"
            name="StarUML View Category">
      </category>
      <view
            category="org.star.uml.designer.ui.views.category.views.staruml"
            class="org.star.uml.designer.ui.views.StarPMSModelView"
            icon="icons/sample.gif"
            id="org.star.uml.designer.ui.views.StarPMSModelView"
            name="StarPMS Model"
            restorable="true">
      </view>
      <view
            category="org.star.uml.designer.ui.views.category.views.staruml"
            class="org.star.uml.designer.ui.views.StarPMSRequestTableView"
            icon="icons/sample.gif"
            id="org.star.uml.designer.ui.views.StarPMSRequestTableView"
            name="StarPMS Request Table"
            restorable="true">
      </view>
      <view
            category="org.star.uml.designer.ui.views.category.views.staruml"
            class="org.star.uml.designer.ui.views.StarPMSBrowersView"
            icon="icons/sample.gif"
            id="org.star.uml.designer.ui.views.StarPMSBrowersView"
            name="StarPMS Browser"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.star.uml.designer.application.Perspective"
            fixed="true"
            icon="icons/16.gif"
            id="org.star.uml.designer.application.perspective"
            name="STAR UML">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.star.uml.designer.application.perspective">
         <view
               closeable="false"
               id="org.eclipse.jdt.ui.PackageExplorer"
               minimized="false"
               moveable="false"
               ratio="0.25f"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.star.uml.designer.ui.views.StarPMSModelView"
               minimized="false"
               moveable="false"
               relationship="stack"
               relative="org.eclipse.jdt.ui.PackageExplorer"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.eclipse.ui.views.PropertySheet"
               minimized="false"
               moveable="false"
               ratio="0.75f"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.star.uml.designer.ui.views.StarPMSRequestTableView"
               minimized="false"
               moveable="false"
               relationship="stack"
               relative="org.eclipse.ui.views.PropertySheet"
               showTitle="true"
               visible="true">
         </view>
         <view
               closeable="false"
               id="org.star.uml.designer.ui.views.StarPMSBrowersView"
               minimized="false"
               moveable="false"
               relationship="stack"
               relative="org.eclipse.ui.views.PropertySheet"
               showTitle="true"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.star.uml.designer.application.application"
            name="StarUML Designer">
         <property
               name="windowImages"
               value="icons/16.gif,icons/32.gif,icons/48.gif,icons/64.gif">
         </property>
         <property
               name="appName"
               value="StarUML Designer">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.uml2.diagram.clazz.org_eclipse_uml2_diagram_clazz_extension_starUML">
      <client
            class="org.eclipse.uml2.diagram.extension.imple.StarUMLExtensionImple">
      </client>
   </extension>
   <extension
         point="org.eclipse.uml2.diagram.sequence.org_eclipse_uml2_diagram_sequence_extension_starUML">
      <client
            class="org.eclipse.uml2.diagram.extension.imple.StarUMLExtensionImple">
      </client>
   </extension>
   <extension
         point="org.eclipse.uml2.diagram.usecase.org_eclipse_uml2_diagram_usecase_extension_starUML">
      <client
            class="org.eclipse.uml2.diagram.extension.imple.StarUMLExtensionImple">
      </client>
   </extension>

</plugin>
